{
  "name": "atfw",
  "version": "1.0.0",
  "description": "automation test framework",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 20000 index.js --reporter mochawesome --reporter-options reportDir=reporting,reportFilename=TestReport",
    "testchrome": "set BROWSER=chrome&& mocha --timeout 15000 index.js --reporter mochawesome --reporter-options reportDir=reporting,reportFilename=TestReport",
    "testfirefox": "set BROWSER=firefox&& mocha --timeout 15000 index.js --reporter mochawesome --reporter-options reportDir=reporting,reportFilename=TestReport"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "chai": "^4.2.0",
    "chromedriver": "^103.0.0",
    "dotenv": "^8.2.0",
    "html-to-json": "^0.6.0",
    "jsdom": "^16.2.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "puppeteer": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "xhr-request": "^1.1.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "mochawesome": "^7.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
